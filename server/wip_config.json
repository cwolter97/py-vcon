{
    "environment": {
        "HOSTNAME":"https://localhost:8000",
        "STITCHER_DATABASE_URL":"",
        "SLACK_TOKEN":"",
        "OPENAI_API_KEY":""
    },
    "links": {
        "stitcher": {
            "module": "plugins.stitcher",
            "options": {
            }
        },
        "call_log": {
            "module": "plugins.call_log",
            "options": {
                "transcribe": true,
                "min_transcription_length": 10,
                "deepgram": false
            }
        },
        "deepgram": {
            "module": "plugins.deepgram",
            "options": {
                "transcribe": true,
                "min_transcription_length": 10,
                "deepgram": false
            }
        }
    },
    "storage": {
        "s3_vcons": {
            "module": "storage:s3",
            "options": {
                "bucket_name": "vons",
                "AWS_KEY": "AWS_KEY"
            }
        },
        "mongo": {
            "module": "storage:mongo",
            "options": {
                "MONGO_URL": "xxxxxxxxxxxxx",
                "DB_NAME": "yyyyyyyyy",
                "COLLECTION": "zzzzzzzzzz"
            }
        }
    },
    "chains": {
        "strolid_ops": {
            "links": ["stitcher", "call_log"],
            "ingress": ["bria"],
            "storage": ["s3_vcons", "mongo"],
            "outbound_lists": "strolid_ops:complete",
            "enabled": 1
        }
    }
}
