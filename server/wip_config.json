{
    "environment": {
        "HOSTNAME":"https://localhost:8000",
        "STITCHER_DATABASE_URL":"",
        "SLACK_TOKEN":"",
        "OPENAI_API_KEY":""
    },
    "links": {
        "transcribe": {
            "module":"links.transcribe",
            "ingress-lists": [],
            "egress-lists": [],  
            "options": {
                "transcribe_options": {
                    "model_size": "base", 
                    "output_options": ["vendor"]
                }
            }
        },
        "script": {
            "module":"links.script",
            "ingress-lists": [],
            "egress-lists": []
        },
        "summary": {
            "module":"links.summary",
            "ingress-lists": [],
            "egress-lists": []
            },
        "tag": {
            "module": "links.tag",
            "ingress-lists": [],
            "egress-lists": [],        
            "options": {
                "tags":["Geddy", "Alex", "The Professor"]
            }
        },
        "webhook": {
            "module": "links.webhook",
            "options": {
                "webhook-urls": ["notreal.com"]
            }
        }
    },
    "storages": {
         "mongo": {
            "module": "storage.mongo",
            "options": {
                "MONGO_URL": "mongodb://localhost:27017/",
                "database": "conserver",
                "collection": "vcons"
            }
        }
    },
    "chains": {
        "sample_chain": {
            "links": ["tag"],
            "ingress_lists": ["test_list"],
            "storages": ["mongo"],
            "egress_lists": ["test_output"],
            "enabled": 1
        }
    }
}
